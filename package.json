    "openui5-xml-loader": "^0.2.0-beta.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "qunitjs": "^2.4.1",
    "raw-loader": "^0.5.1",
    "script-loader": "^0.7.2",
    "sinon": "^4.4.5",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
