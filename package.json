{
  "name": "openui5-sample-app",
  "version": "0.1.0",
  "description": "Sample of an OpenUI5 app",
  "private": true,
  "scripts": {
    "test": "grunt lint && grunt test",
    "start": "grunt serve",
    "build": "grunt build",
    "deploy": "npm run webpack:prod && gh-pages -d dist",
    "postinstall": "bower install",
    "lint": "eslint webapp",
    "server": "webpack-dev-server",
    "webpack": "webpack --progress",
    "webpack:prod": "cross-env NODE_ENV=production webpack --progress --config webpack.config.prod.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-openui5-support": "^0.1.0-beta.0",
    "babel-preset-env": "^1.6.1",
    "bower": "^1.8.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.0.6",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-openui5": "~0.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-qunit": "^1.2.1",
    "karma-sinon": "^1.0.5",
